execute if score @s expai.jump_gap_roof_least_amount matches 0..4 if score @s expai.jump_gap_roof_most_amount matches 0..4 if score @s expai.jump_gap_length matches 2..8 as @e[tag=Jump_Direction,limit=1,sort=nearest] run tag @s add expai.jump_gap_roofed
execute if score @s expai.jump_gap_roof_least_amount matches 5..6 if score @s expai.jump_gap_roof_most_amount matches 5..6 if score @s expai.jump_gap_length matches 9..12 as @e[tag=Jump_Direction,limit=1,sort=nearest] run tag @s add expai.sprint_jump_gap_roofed